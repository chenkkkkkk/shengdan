pan>\r\n        <Icon type=\"md-menu\" size=\"30\" style=\"height:30px;\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <h1 style=\"color:red\">ajsdasd就安静了法拉盛的分解落实第九届立法</h1>\r\n  \r\n</template>\r\n\r\n<script>\r\nimport IMG_1555 from \"../assets/music/IMG_1555.mp3\"\r\nimport video from \"../assets/music/video.mp3\"\r\nexport default {\r\n  name: \"myXiangCe\",\r\n  data() {\r\n    return {\r\n      flag: true,\r\n      music_data: [IMG_1555, video],\r\n      timer: '',\r\n      t: '',\r\n      url: \"../assets/music/\",\r\n      my_src: \"\"\r\n    }\r\n  },\r\n  methods: {\r\n    shenSuo() {\r\n      let disc = this.$refs.bt_yinyue\r\n      console.log()\r\n      if (disc.offsetWidth == 30) {\r\n        let music1 = document.querySelector(\".music1\")\r\n        disc.classList.toggle(\"big\")\r\n        music1.classList.toggle(\"big1\")\r\n      }\r\n    },\r\n    onPlay() {\r\n      let fff = document.querySelector(\"#vd1\").firstElementChild\r\n      console.log(fff)\r\n      console.log(\"开始播放\"+fff.src)\r\n      this.flag = true\r\n    },\r\n    onPause() {\r\n      console.log(\"暂停播放\")\r\n      this.flag = false\r\n    },\r\n    overAudio() {\r\n      console.log(\"结束播放\")\r\n      let index = this.music_data.indexOf(this.my_src)\r\n      console.log(\"index: \" + index)\r\n      index = index + 1\r\n      if (index >= this.music_data.length) {\r\n        index = 0\r\n      }\r\n      this.my_src = this.music_data[index]\r\n      let audio1 = document.querySelector(\"#vd1\");\r\n      audio1.load();\r\n    },\r\n    boFang() {\r\n      let audio1 = document.querySelector(\"#vd1\");\r\n      if (audio1.paused) {\r\n        audio1.play();\r\n      } else {\r\n        audio1.pause();\r\n      }\r\n    },\r\n    //上一首\r\n    topp() {\r\n      console.log(\"上一首\")\r\n      let index = this.music_data.indexOf(this.my_src)\r\n      index -= 1\r\n      if (index < 0) {\r\n        index = this.music_data.length - 1\r\n      }\r\n      this.my_src = this.music_data[index]\r\n      let audio1 = document.querySelector(\"#vd1\");\r\n      audio1.load();\r\n    },\r\n    //下一首\r\n    bott() {\r\n      console.log(\"下一首\") \r\n      let index = this.music_data.indexOf(this.my_src)\r\n      console.log(\"index: \" + index)\r\n      index = index + 1\r\n      if (index >= this.music_data.length) {\r\n        index = 0\r\n      }\r\n      this.my_src = this.music_data[index]\r\n      let audio1 = document.querySelector(\"#vd1\");\r\n      audio1.load();\r\n    },\r\n    dingShi() {\r\n      //判断上一次定时器是否执行完\r\n      if (this.t) {\r\n        // console.log(\"t是谁那么\",this.t)\r\n        clearTimeout(this.t)\r\n      }\r\n\r\n      // let disc = this.$refs.bt_yinyue.offsetWidth\r\n      this.t = setTimeout(() => {\r\n          // if (disc == 30) {\r\n            let disc = document.querySelector(\"#disc\")\r\n            let music1 = document.querySelector(\".music1\")\r\n            disc.classList.toggle(\"big\")\r\n            music1.classList.toggle(\"big1\")\r\n          // }\r\n      },5000)\r\n      // console.log(\"宽度\", disc.style.offsetWidth)\r\n      // if (disc.style.width == \"310px\") {\r\n      //   console.log(\"阿什顿\")\r\n      //   disc.style.width == \"30px\"\r\n      // }\r\n    },\r\n    \r\n  },\r\n  created() {\r\n    history.pushState(null, null, document.URL);\r\n    window.addEventListener(\"popstate\", function () {\r\n      history.pushState(null, null, document.URL);\r\n    });\r\n  },\r\n  mounted() {\r\n    let audio1 = document.querySelector(\"#vd1\");\r\n    if (audio1.paused) {\r\n      this.flag = false;\r\n    } else {\r\n      this.flag = true;\r\n    }\r\n    let music_data = this.music_data;\r\n    var index = Math.floor((Math.random() * music_data.length));\r\n    console.log(music_data[index]);\r\n    // let sui_ji_yin_yue = this.$refs.audio\r\n    // console.log(sui_ji_yin_yue);\r\n    this.my_src = music_data[index]\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.bigbox {\r\n  wi