.flag = false;\n    } else {\n      this.flag = true;\n    }\n    console.log(\"来了没啊\");\n  }\n};","map":{"version":3,"mappings":"AAuBA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,IAAI;MACVC,UAAU,EAAE,CACV;QACEC,EAAE,EAAE,CAAC;QACLC,GAAG,EAAE,wBAAwB;QAC7BC,OAAO,EAAC;MACV,CAAC,EACD;QACEF,EAAE,EAAE,CAAC;QACLC,GAAG,EAAE,qBAAqB;QAC1BC,OAAO,EAAE;MACX,CAAC,CACF;MACDC,KAAK,EAAE,EAAE;MACTC,CAAC,EAAC;IACJ;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,GAAG;MACR,IAAIC,IAAG,GAAIC,QAAQ,CAACC,aAAa,CAAC,OAAO;MACzC,IAAIC,MAAK,GAAIF,QAAQ,CAACC,aAAa,CAAC,SAAS;MAC7CF,IAAI,CAACI,SAAS,CAACC,MAAM,CAAC,KAAK;MAC3BF,MAAM,CAACC,SAAS,CAACC,MAAM,CAAC,MAAM;IAChC,CAAC;IACDC,MAAM,GAAG;MACPC,OAAO,CAACC,GAAG,CAAC,MAAM;MAClB,IAAI,CAACjB,IAAG,GAAI,IAAG;IACjB,CAAC;IACDkB,OAAO,GAAG;MACRF,OAAO,CAACC,GAAG,CAAC,MAAM;MAClB,IAAI,CAACjB,IAAG,GAAI,KAAI;IAClB,CAAC;IACDmB,SAAS,GAAG;MACVH,OAAO,CAACC,GAAG,CAAC,MAAM;MAClB,IAAI,CAAChB,UAAU,CAACmB,OAAO,CAACC,IAAG,IAAK;QAC9BA,IAAI,CAACjB,OAAM,GAAI,IAAI;MACrB,CAAC;IACH,CAAC;IACDkB,MAAM,GAAG;MACP,IAAIC,MAAK,GAAIb,QAAQ,CAACC,aAAa,CAAC,MAAM,CAAC;MAC3C,IAAIY,MAAM,CAACC,MAAM,EAAE;QACjBD,MAAM,CAACE,IAAI,EAAE;MACf,OAAO;QACLF,MAAM,CAACG,KAAK,EAAE;MAChB;IACF,CAAC;IACDC,OAAO,GAAG;MACR,IAAIC,EAAC,GAAI,IAAI,CAACtB;MACdsB,EAAC,GAAIC,UAAU,CAAED,EAAE,IAAK;QACtBZ,OAAO,CAACC,GAAG,CAAC,MAAM,CAAC;QACnB,IAAIR,IAAG,GAAIC,QAAQ,CAACC,aAAa,CAAC,OAAO;QACzC,IAAIC,MAAK,GAAIF,QAAQ,CAACC,aAAa,CAAC,SAAS;QAC7CF,IAAI,CAACI,SAAS,CAACC,MAAM,CAAC,KAAK;QAC3BF,MAAM,CAACC,SAAS,CAACC,MAAM,CAAC,MAAM;QAC9BgB,YAAY,CAACF,EAAE,CAAC;MAClB,CAAC,EAAE,IAAI,CAAC;;MAER;MACA;MACA;MACA;MACA;MACA;MACA;IACF;EACF,CAAC;;EACDG,OAAO,GAAG;IACRC,OAAO,CAACC,SAAS,CAAC,IAAI,EAAE,IAAI,EAAEvB,QAAQ,CAACwB,GAAG,CAAC;IAC3CC,MAAM,CAACC,gBAAgB,CAAC,UAAU,EAAE,YAAY;MAC9CJ,OAAO,CAACC,SAAS,CAAC,IAAI,EAAE,IAAI,EAAEvB,QAAQ,CAACwB,GAAG,CAAC;IAC7C,CAAC,CAAC;EACJ,CAAC;EACDG,OAAO,GAAG;IACR,IAAId,MAAK,GAAIb,QAAQ,CAACC,aAAa,CAAC,MAAM,CAAC;IAC3C,IAAIY,MAAM,CAACC,MAAM,EAAE;MACjB,IAAI,CAACxB,IAAG,GAAI,KAAK;IACnB,OAAO;MACL,IAAI,CAACA,IA